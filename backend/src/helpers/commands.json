{
  "GIT": {
    "CLONE_REPO": "git clone https://github.com/<account_name>/<repository_name>.git",
    "ADD_FILES": "git add <filenames> || git add ./",
    "COMMIT": "git commit -m '<commit_message>'",
    "PUSH": "git push origin <branch_name>",
    "PULL": "git pull origin <branch_name>",
    "BRANCH_CREATE": "git branch <new_branch_name>",
    "BRANCH_SWITCH": "git checkout <branch_name>",
    "BRANCH_CREATE_AND_SWITCH": "git checkout -b <new_branch_name>",
    "STATUS": "git status",
    "LOG": "git log",
    "STASH": "git stash",
    "STASH_APPLY": "git stash apply",
    "MERGE": "git merge <branch_name>",
    "DIFF": "git diff <branch1>..<branch2>",
    "REBASE": "git rebase <branch_name>",
    "RESET_SOFT": "git reset --soft HEAD~<number_of_commits>",
    "RESET_HARD": "git reset --hard HEAD~<number_of_commits>",
    "DISCARD_CHANGES": "git checkout -- <filename> || git restore <filename>",
    "DELETE_BRANCH": "git branch -d <branch_name>",
    "REMOTE_ADD": "git remote add origin https://github.com/<account_name>/<repository_name>.git",
    "SHOW_REMOTE": "git remote -v",
    "TAG": "git tag <tag_name>",
    "SHOW_TAGS": "git tag",
    "DELETE_TAG": "git tag -d <tag_name>",
    "CHERRY_PICK": "git cherry-pick <commit_hash>",
    "FETCH": "git fetch",
    "PULL_REBASE": "git pull --rebase origin <branch_name>",
    "CONFIG_GLOBAL_USER": "git config --global user.name '<username>' && git config --global user.email '<email>'",
    "CREATE_REPO": "git init",
    "REMOVE_FILE_FROM_REPO": "git rm <filename>",
    "REMOVE_CACHED_FILE": "git rm --cached <filename>",
    "IGNORE_FILE": "echo '<filename>' >> .gitignore",
    "SHOW_CURRENT_BRANCH": "git rev-parse --abbrev-ref HEAD"
  },
  "MARKDOWN": {
    "HEADINGS": "# Heading 1\n## Heading 2\n### Heading 3",
    "BOLD": "**This text is bold**",
    "ITALICS": "*This text is italic*",
    "STRIKETHROUGH": "~~This text is strikethrough~~",
    "UNORDERED_LIST": "- Item 1\n- Item 2\n  - Subitem 1\n  - Subitem 2",
    "ORDERED_LIST": "1. Item 1\n2. Item 2\n3. Item 3",
    "LINK": "[Link Text](https://example.com)",
    "IMAGE": "![Alt Text](https://example.com/image.png)",
    "INLINE_CODE": "`Inline code snippet`",
    "CODE_BLOCK": "```language\nCode block\n```",
    "BLOCKQUOTE": "> This is a blockquote",
    "TABLE": "| Column 1 | Column 2 | Column 3 |\n|----------|----------|----------|\n| Value 1  | Value 2  | Value 3  |",
    "HORIZONTAL_RULE": "---",
    "TASK_LIST": "- [ ] Task 1\n- [x] Completed Task",
    "ESCAPE_CHARACTERS": "\\*Escape special characters\\*"
  }
}
